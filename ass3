 }
 struct binarytree *Initialize()
 {
    root=NUll;
 }
 struct binarytree *makeempty(struct binarytree *root)
 {
    if(root!=NUll)
    {
        makeempty(root->lchild)
        makeempty(root->rchild)
        delete root;
    }
    return NULL;
 }

